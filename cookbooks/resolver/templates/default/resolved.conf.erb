#
# Generated by Chef Infra for <%= node['fqdn'] %>
# Do NOT modify this file by hand, changes will be overwritten.
#

[Resolve]
<% unless nil_or_empty?(@dns) -%>
DNS=<%= @dns.join(' ') %>
<% end -%>
<% unless nil_or_empty?(@fallbackdns) -%>
FallBackDNS=<%= @fallbackdns.join(' ') %>
<% end -%>
<% unless nil_or_empty?(@domains) -%>
Domains=<%= @domains.join(' ') %>
<% end -%>
<% unless nil_or_empty?(@llmnr) -%>
LLMNR=<%= systemd_value(@llmnr) %>
<% end -%>
<% unless nil_or_empty?(@multicastdns) -%>
MulticastDNS=<%= systemd_value(@multicastdns) %>
<% end -%>
<% unless nil_or_empty?(@dnssec) -%>
DNSSEC=<%= systemd_value(@dnssec )%>
<% end -%>
<% unless nil_or_empty?(@dnsovertls) -%>
DNSOverTLS=<%= systemd_value(@dnsovertls) %>
<% end -%>
<% unless nil_or_empty?(@cache) -%>
Cache=<%= systemd_value(@cache) %>
<% end -%>
<% unless nil_or_empty?(@cachefromlocalhost) -%>
CacheFromLocalhost=<%= systemd_value(@cachefromlocalhost)%>
<% end -%>
<% unless nil_or_empty?(@dnsstublistener) -%>
DNSStubListener=<%= systemd_value(@dnsstublistener) %>
<% end -%>
<% unless nil_or_empty?(@dnsstublistenerextra) -%>
<% @dnsstublistenerextra.each do |dslx| -%>
DNSStubListenerExtra=<%= dslx.strip %>
<% end -%>
<% end -%>
<% unless nil_or_empty?(@readetchosts) -%>
ReadEtcHosts=<%= systemd_value(@readetchosts) %>
<% end -%>
<% unless nil_or_empty?(@resolveunicastsinglelabel) -%>
ResolveUnicastSingleLabel=<%= systemd_value(@resolveunicastsinglelabel) %>
<% end -%>
